        <addForeignKeyConstraint baseColumnNames="sucursal_id"
                                 baseTableName="oficina"
                                 constraintName="fk_oficina__sucursal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sucursal"/>
    </changeSet>
</databaseChangeLog>
